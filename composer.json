{
    "name": "amore-roberto/woocommerce-timeflow-delivery",
    "description": "WooCommerce extension for delivery date and time slot selection",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "autoload": {
        "psr-4": {
            "WooCommerce\\TimeFlow\\Delivery\\": "includes/"
        }
    },
    "require": {
        "php": ">=7.4",
        "composer/installers": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.6",
        "wp-coding-standards/wpcs": "^2.3"
    },
    "scripts": {
        "test": "phpunit",
        "phpcs": "phpcs --standard=WordPress"
    }
} 